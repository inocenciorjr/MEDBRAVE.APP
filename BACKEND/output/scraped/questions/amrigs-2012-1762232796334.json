        "explanation": null,
        "order": 2
      },
      {
        "id": "q-1ee91468-7-alt-3",
        "text": "D ) Apenas I e III.",
        "isCorrect": false,
        "explanation": null,
        "order": 3
      },
      {
        "id": "q-1ee91468-7-alt-4",
        "text": "E ) I, II e III.",
        "isCorrect": true,
        "explanation": null,
        "order": 4
      }
