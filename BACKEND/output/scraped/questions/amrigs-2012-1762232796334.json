      },
      {
        "id": "q-1ee91468-7-alt-1",
        "text": "B ) Apenas III.",
        "isCorrect": false,
        "explanation": null,
        "order": 1
      },
      {
        "id": "q-1ee91468-7-alt-2",
        "text": "C ) Apenas I e II.",
