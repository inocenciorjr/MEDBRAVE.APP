        "order": 1
      },
      {
        "id": "q-cabebf64-9-alt-2",
        "text": "C ) Cardioversão elétrica.",
        "isCorrect": false,
        "explanation": null,
        "order": 2
      },
      {
        "id": "q-cabebf64-9-alt-3",
        "text": "D ) Metoprolol.",
        "isCorrect": true,
        "explanation": null,
        "order": 3
      },
      {
        "id": "q-cabebf64-9-alt-4",
        "text": "E ) Amiodarona.",
        "isCorrect": false,
        "explanation": null,
        "order": 4
      }
    ],
