{
  "name": "medforum",
  "version": "1.0.0",
  "description": "Plataforma médica moderna e segura para educação médica continuada",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/server.ts",
    "worker": "cross-env WORKER_MODE=true ts-node-dev --respawn --transpile-only --ignore-watch node_modules --no-notify src/worker.ts",
    "build": "rimraf dist && tsc",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint --config eslint.config.mjs . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "echo 'Skipping husky in production'",
    "pre-commit": "lint-staged",
    "docs": "typedoc --out docs src",
    "analyze-indexes": "ts-node scripts/analyze-required-indexes.ts",
    "create-indexes": "ts-node scripts/create-indexes.ts",
    "setup-exec-function": "ts-node scripts/setup-exec-function.ts",
    "analyze-composite-indexes": "ts-node analyze-missing-composite-indexes.ts",
    "scrape": "ts-node scripts/scraper/index.ts",
    "scrape:hardworq": "ts-node scripts/scraper-hardworq/index.ts",
    "scrape:hardworq:list": "ts-node scripts/scraper-hardworq/list-provas.ts",
    "clean-r2": "ts-node scripts/clean-r2-bucket.ts",
    "nuke-r2": "ts-node scripts/nuke-r2.ts",
    "nuke-flashcards-r2": "ts-node scripts/nuke-flashcards-r2.ts",
    "test:qwen": "ts-node scripts/test-qwen.ts",
    "redis:list": "ts-node --transpile-only scripts/redis-jobs-manager.ts list",
    "redis:clean": "ts-node --transpile-only scripts/redis-jobs-manager.ts clean",
    "redis:obliterate": "ts-node --transpile-only scripts/redis-jobs-manager.ts obliterate",
    "redis:remove": "ts-node --transpile-only scripts/redis-jobs-manager.ts remove",
    "test:glm": "ts-node scripts/test-glm.ts",
    "test:minimax": "ts-node scripts/test-minimax.ts"
  },
  "keywords": [
    "medical",
    "education",
    "typescript",
    "supabase",
    "postgresql",
    "srs",
    "spaced-repetition"
  ],
  "author": "",
  "license": "ISC",
