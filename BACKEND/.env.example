# Supabase
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Server
PORT=3000
NODE_ENV=development

# Payment
ALLOWED_CURRENCIES=BRL,USD,EUR
DEFAULT_CURRENCY=BRL

# Cache
CACHE_ENABLED=true
CACHE_TTL_PUBLIC_PLANS=300
CACHE_TTL_PLAN_BY_ID=600

# Rate Limiting
RATE_LIMIT_ENABLED=true
RATE_LIMIT_PUBLIC_PLANS_WINDOW_MS=60000
RATE_LIMIT_PUBLIC_PLANS_MAX=30
RATE_LIMIT_CREATE_PLAN_WINDOW_MS=60000
RATE_LIMIT_CREATE_PLAN_MAX=10
RATE_LIMIT_UPDATE_PLAN_WINDOW_MS=60000
RATE_LIMIT_UPDATE_PLAN_MAX=20

# Jobs
EXPIRE_PLANS_JOB_ENABLED=true
EXPIRE_PLANS_JOB_INTERVAL=3600000

# Logging
LOG_LEVEL=info
