R2_SECRET_ACCESS_KEY=sua-secret-access-key
R2_ENDPOINT=https://seu-account-id.r2.cloudflarestorage.com
R2_PUBLIC_URL=https://seu-dominio.com.br
VITE_R2_PUBLIC_URL=https://seu-dominio.com.br
